λ even n ret_true ret_false
	is_zero n ret_true recurse
	λ recurse nil
		pred n recurse' nil
		λ recurse' n'
			odd n' ret_true ret_false

λ odd n ret_true ret_false
	is_zero n ret_false recurse
	λ recurse nil
		pred n recurse' nil
		λ recurse' n'
			even n' ret_true ret_false

